<Window x:Class="Mini.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mini.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Origem" HorizontalAlignment="Center" Margin="0,77,0,0" TextWrapping="Wrap" Text="inicial.txt" VerticalAlignment="Top" Width="214" Height="26"/>
        <TextBox x:Name="Destino" HorizontalAlignment="Center" Margin="0,138,0,0" TextWrapping="Wrap" Text="final.txt" VerticalAlignment="Top" Width="214" Height="26"/>
        <Button x:Name="btnExec" Content="Executar" HorizontalAlignment="Center" VerticalAlignment="Center" Height="23" Width="96" Click="btnExec_Click"/>

    </Grid>
</Window>
